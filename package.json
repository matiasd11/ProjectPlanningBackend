{
  "name": "project-planning-backend",
  "version": "1.0.0",
  "description": "Backend Express + MySQL con Sequelize para gesti√≥n de proyectos",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f backend",
    "docker:build": "docker-compose build",
    "docker:restart": "docker-compose restart backend",
    "docker:prod": "NODE_ENV=production DOCKER_COMMAND='npm start' docker-compose up -d",
    "db:reset": "docker-compose exec mysql mysql -u root -proot -e 'DROP DATABASE IF EXISTS projectplanning; CREATE DATABASE projectplanning;' && docker-compose restart backend"
  },
  "keywords": [
    "express",
    "mysql",
    "sequelize",
    "docker",
    "api",
    "backend"
  ],
  "author": "Matias D",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.0.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.6.5",
    "sequelize": "^6.35.1"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.3"
  }
